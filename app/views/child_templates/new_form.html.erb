<h1>
  Add a new child
</h1>


<hr>

<form action="/create_child" method="post">

  <div class="form-group">
    <label for="children_id">
      Children
    </label>

    <input type="text" id="children_id" name="children_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="children_date">
      Children date
    </label>

    <input type="text" id="children_date" name="children_date" class="form-control">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create child
  </button>
</form>

<hr>

<a href="/children">
  Go back
</a>
